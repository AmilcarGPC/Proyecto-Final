    "reader = LectorArchivoPython(\"input.py\")\n",
    "is_valid, error = reader.validar()\n",
    "if not is_valid:\n",
    "    print(f\"Error: {error}\")\n",
    "else:\n",
    "    code, error = reader.leer_lineas()\n",
    "\n",
    "new_code = []\n",
    "for line in code:\n",
    "    formatted = LineFormatter.format_line(line)\n",
    "    if len(formatted) > 1:\n",
    "        new_code.extend(formatted)\n",
    "    else:\n",
    "        for f in formatted:\n",
    "            new_code.append(f)\n",
    "\n",
    "tree = ArbolArchivoPython(new_code)\n",
    "\n",
    "is_valid, error = es_arbol_sintactico_valido(tree.raiz)\n",
    "\n",
    "if not is_valid:\n",
    "    print(f\"Error: {error}\")\n",
    "else:\n",
    "    Pcounter = ContadorLineasFisicas()\n",
    "    Lcounter = ContadorLineasLogicas()\n",
    "    physical_lines = Pcounter.count_physical_lines(tree.raiz)\n",
    "    logical_lines = Lcounter.contar_lineas_logicas(tree.raiz)\n",
    "\n",
    "    print(f\"Physical lines: {physical_lines}\")\n",
    "    print(f\"Logical lines: {logical_lines}\")"
   ]
